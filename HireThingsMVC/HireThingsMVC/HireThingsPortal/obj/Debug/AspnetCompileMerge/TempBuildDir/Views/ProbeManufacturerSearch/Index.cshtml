@model MEPS.MEPSTemp.Model.ProbeManufacturerSearchViewModel
@{
    ViewBag.Title = "Probe Manufacture";
}

@using (Html.BeginForm("IndexEdit", "ProbeManufacturer", FormMethod.Post, new { id = "ProbeManfSearch" }))
{
    @Html.Hidden("hdProbeManfId")
   

                    <div class="row">
                        <div class="col-lg-12">
                            <div class="panel CustomSearchBox">
                                <div class="panel-body">
                                <div class="">
                                    <div class="form-group">
                                        <div class="col-sm-12">
                                            <label class="control-label">Search</label>
                                            <input type="text" class="form-control" id="txtSearch" />
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <div class="col-sm-6">
                                            <span class="visible-md visible-sm visible-xs">&nbsp;</span>
                                            <input type="button" class="btn btn-primary" value="Search" id="btnProbeManfSearch" />
                                        </div>
                                    </div>
                                </div>                        
                            </div>
                            </div>
                        </div>
                    </div>
                        
                        
                    <div id="DivProbeManfGrid">
                        @Html.Partial("_ProbeManfGrid", Model.ManufList)
                    </div>

                    <script src="~/Scripts/AppJS/probmanufacturer/mepstemp.probmanufacture.search.js"></script>
                    <script>
        $(document).ready(function () {
            MEPSTEMP.UI.ProbeManf.Search.initialisePage();
        });
                    </script>
  
                    
			
}            <!-- Content End -->