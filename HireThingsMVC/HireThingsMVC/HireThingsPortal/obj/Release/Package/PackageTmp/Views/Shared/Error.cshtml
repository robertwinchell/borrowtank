@*model System.Web.Mvc.HandleErrorInfo*@

@{
    ViewBag.Title = "Error";
}

<div class="row   mb-sm">
    <div class="col-lg-12">
        <div class="alert alert-danger mb-sm">
            <p class="m-none text-semibold h6">
                @if (ViewData["CustomSuccessMessage"] != null)
                {
                    @Html.Raw(ViewData["CustomSuccessMessage"]);
                }
                else
                {
                    @Html.Raw(" Error : Record cannot  be Saved.");
                    @Html.ValidationSummary()
                }

            </p>
        </div>
    </div>
</div>